{
  ":configurator:resource-version": 1,
  "com.adeptj.modules.jaxrs.resteasy.internal.ResteasyLifecycle": {
    "corsMaxAge": 86400,
    "allowCredentials": true,
    "allowedMethods": [
      "GET",
      "POST",
      "PUT",
      "DELETE",
      "OPTIONS",
      "HEAD"
    ],
    "allowedHeaders": [
      "Cache-Control",
      "Content-Language",
      "Content-Length",
      "Content-Type",
      "Expires",
      "Last-Modified",
      "Pragma"
    ],
    "exposedHeaders": [
      "Content-Length",
      "Authorization"
    ],
    "allowedOrigins": [
      "*"
    ],
    "providerDenyList": [
      "org.jboss.resteasy.plugins.validation.ValidatorContextResolver",
      "org.jboss.resteasy.plugins.validation.ValidatorContextResolverCDI"
    ],
    "sendExceptionTrace": true
  }
}