{
  ":configurator:resource-version": 1,
  "com.adeptj.modules.jaxrs.resteasy.internal.ResteasyLifecycle": {
    "cors.max.age": 86400,
    "allow.credentials": true,
    "allowed.methods": [
      "GET",
      "POST",
      "PUT",
      "DELETE",
      "OPTIONS",
      "HEAD"
    ],
    "allowed.headers": [
      "Cache-Control",
      "Content-Language",
      "Content-Length",
      "Content-Type",
      "Expires",
      "Last-Modified",
      "Pragma"
    ],
    "exposed.headers": [
      "Content-Length",
      "Authorization"
    ],
    "allowed.origins": [
      "*"
    ],
    "provider.skip.list": [
      "org.jboss.resteasy.plugins.validation.ValidatorContextResolver",
      "org.jboss.resteasy.plugins.validation.ValidatorContextResolverCDI"
    ],
    "send.exception.trace": true,
    "log.web.application.exception": true
  }
}