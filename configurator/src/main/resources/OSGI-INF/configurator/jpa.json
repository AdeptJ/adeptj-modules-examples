{
  ":configurator:resource-version": 1,
  "com.adeptj.modules.data.jpa.eclipselink.internal.EntityManagerFactoryLifecycle": {
    "persistence.xml.location": "META-INF/persistence.xml",
    "deploy.on.startup": "true",
    "use.exception.handler": true,
    "exception.handler.fqcn": "com.adeptj.modules.data.jpa.eclipselink.extension.JpaExceptionHandler",
    "log.query.parameters": true,
    "query.retry.attempt.count": 0,
    "persistence.unit.transaction.type": "RESOURCE_LOCAL",
    "shared.cache.mode": "ENABLE_SELECTIVE",
    "validation.mode": "CALLBACK",
    "logging.level": "FINEST",
    "eclipselink.loggers": [
      "eclipselink.logging.level.connection",
      "eclipselink.logging.level.jpa",
      "eclipselink.logging.level.sequencing",
      "eclipselink.logging.level.sql",
      "eclipselink.logging.level.query",
      "eclipselink.logging.level.cache",
      "eclipselink.logging.level.transaction",
      "eclipselink.logging.level.weaver",
      "eclipselink.logging.level.metadata",
      "eclipselink.logging.level.metamodel",
      "eclipselink.logging.level.properties",
      "eclipselink.logging.level.ddl",
      "eclipselink.logging.level.server",
      "eclipselink.logging.level.event",
      "eclipselink.logging.level.misc"
    ],
    "eclipselink.slf4j.logger.fqcn": "com.adeptj.modules.data.jpa.eclipselink.extension.SLF4JLogger",
    "ddl.generation.output.mode": "both",
    "ddl.generation": "create-or-extend-tables",
    "jpa.properties": []
  }
}